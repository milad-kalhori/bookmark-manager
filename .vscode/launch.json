    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Program",
        "program": "${workspaceFolder}/app.js",
        "env": {
          "NODE_ENV": "development",
          "PORT": "8000",
          "DATABASE_LOCAL": "mongodb://localhost:27017/books-directory"
        },
        "request": "launch",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "type": "node"
      },
      {
        "name": "Launch Program 2",
        "program": "${workspaceFolder}\\app.js",
        "env": {
          "NODE_ENV": "development",
          "PORT": "5000",
          "DATABASE_LOCAL": "mongodb://localhost:27017/books-directory"
        },
        "type": "node",
        "request": "launch",
        "skipFiles": [
            "<node_internals>/**"
        ],
      }
    ]
}